{
    "extends": "airbnb-base",
    "plugins": ["import"],
    "env": {
      "node": true,
      "es2021": true
    },
    "parserOptions": {
      "ecmaVersion": 2021
    },
    "rules": {
      "semi": ["error", "never"],
      "quotes": ["error", "single"], 
      "indent": ["error", 2] 
    }
  }
  